{
  "name": "lyrics-wave",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "schema:download": "npx apollo service:download --endpoint=http://localhost:4000/graphql ./src/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"
  },
  "dependencies": {
    "node-sass": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/redux-logger": "^3.0.9",
    "@vitejs/plugin-react": "^1.0.7",
    "apollo": "^2.33.4",
    "apollo-codegen-core": "0.40.3",
    "apollo-language-server": "^1.26.3",
    "autoprefixer": "^10.4.2",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.6",
    "react-hook-form": "^7.27.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.57.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
